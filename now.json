{
  "version": 2,
  "regions": [	
    "all"	
  ],
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DEBUG": "microlink-proxy*"
  },
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
