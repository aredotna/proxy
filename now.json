{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DOMAINS": "@microlink-domains",
    "API_KEY": "@microlink-api-key"
  },
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
